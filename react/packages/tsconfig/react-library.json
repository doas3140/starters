{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "jsx": "react-jsx",
    "lib": ["ES2015"],
    "module": "esnext",
    "noEmit": true,
    "strict": false,
    "target": "es6"
  },
  "exclude": ["node_modules"]
}
